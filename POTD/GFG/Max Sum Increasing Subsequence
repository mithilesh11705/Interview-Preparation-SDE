class Solution {
  public:
    int maxSumIS(vector<int>& arr) {
            int n = arr.size();
      vector<int> dp(n+1, 0);
      
      int ans = 0;
      
      for(int i=0;i<n;i++){
        int s=0;
        for(int j=0;j<i;j++){
          if(arr[i] > arr[j])
            s = max(s, dp[j]);
        }
        
        dp[i] = arr[i] + s;
        ans = max(ans, dp[i]);
      }
      
      return ans;
    }
};
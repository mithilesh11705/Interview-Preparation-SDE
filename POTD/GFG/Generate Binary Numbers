class Solution {
  public:
    vector<string> generateBinary(int n) {
        // code here
        
        queue<string>q;
        q.push("1");
        vector<string>ans;
        
        for(int i=1; i<=n; i++){
            string t=q.front();
            q.pop();
            ans.push_back(t);
            q.push(t+"0");
            q.push(t+"1");
        }
        
        return ans;
    }
};
class Solution {
public:
    int compareVersion(string v1, string v2) {
        vector<string> s1, s2;
        string s = "";

    
        for (char ch : v1) {
            if (ch == '.') {
                s1.push_back(s);
                s = "";
            } else {
                s += ch;
            }
        }
        s1.push_back(s);

   
        s = "";
        for (char ch : v2) {
            if (ch == '.') {
                s2.push_back(s);
                s = "";
            } else {
                s += ch;
            }
        }
        s2.push_back(s);

        int k1 = s1.size(), k2 = s2.size();

   
        if (k1 < k2) {
            int diff = k2 - k1;
            while (diff-- > 0) s1.push_back("0");
        } else if (k2 < k1) {
            int diff = k1 - k2;
            while (diff-- > 0) s2.push_back("0");
        }

 
        for (int i = 0; i < s1.size(); i++) {
            int l1 = stoi(s1[i]);
            int l2 = stoi(s2[i]);
            if (l1 < l2) return -1;
            if (l1 > l2) return 1;
        }
        return 0;
    }
};


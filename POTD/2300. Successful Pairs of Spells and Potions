class Solution {
public:
    using LL = long long;
    vector<int> successfulPairs(vector<int>& spells, vector<int>& potions, long long success) {
        int n = spells.size();
        int m = potions.size();
        sort(potions.begin(), potions.end());
        
        vector<int> res;
        for (int i = 0; i < n; i++) {
            long long k = spells[i];
            long long c = 0;
            
            int l = 0, r = m - 1;
            while (l <= r) {
                int mid = (l + r) / 2;
                long long p = k * 1LL * potions[mid];
                
                if (p >= success) {
                    r = mid - 1;
                } else {
                    l = mid + 1;
                }
            }
            
            c = m - l;
            res.push_back((int)c);
        }
        return res;
    }
};

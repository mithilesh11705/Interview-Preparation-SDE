class Solution {
public:
    int countPartitions(vector<int>& nums) {
        int n=nums.size();

      
        int s=0;
        int t=accumulate(nums.begin(),nums.end(),0);
        int c=0;
        for(int i=0;i<n-1;i++)
        {
            s+=nums[i];
            if(abs(t-2*s) % 2==0) 
            c++;
        }

        return c;

    }
};
class Solution {
public:
    bool hasSameDigits(string s) {
   
        while (s.size() > 2) {
            string ns = "";
            for (int i = 0; i + 1 < s.size(); i++) {
                int a = s[i] - '0';
                int b = s[i + 1] - '0';
                int sum = (a + b) % 10;
                ns += (sum + '0');  
            }
            s = ns;
        }

        return s[0] == s[1];
    }
};

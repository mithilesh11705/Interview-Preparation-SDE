class Solution {
public:
    vector<int> getSneakyNumbers(vector<int>& nums) {
        
        int n=nums.size();

        unordered_map<int,int>mp;
        vector<int>ans;
        for(auto i:nums)
        {
            if(mp.find(i) !=mp.end())
            {
            ans.push_back(i);
            if(ans.size()>2)
            break;
            }

            mp[i]++;
        }

        return ans;
    }
};
class Solution {
  public:
    int findMinInsertions(string &s) {
        // code here
        int n=s.size();
        string k=s;
        reverse(s.begin(),s.end());
        
        vector<vector<int>>dp(n+1,vector<int>(n+1));
        
        for(int i=0;i<=n;i++)
        {
            for(int j=0;j<=n;j++)
            {
                if(i==0 || j==0)
                dp[i][j]=0;
            }
        }
        
        for(int i=1;i<=n;i++)
        {
            for(int j=1;j<=n;j++)
            {
                if(s[i-1]==k[j-1])
                dp[i][j]=1+dp[i-1][j-1];
                else
                dp[i][j]=max(dp[i-1][j],dp[i][j-1]);
            }
        }
        //if(dp[n][n]==0) return n-1;
        return (n-dp[n][n]);
    }
};



no. of deletion == no. of insertion
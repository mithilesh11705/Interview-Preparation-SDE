class Solution {
public:
    bool partitionArray(vector<int>& nums, int k) {
        int n=nums.size();
        if(n%k!=0) return false;

        unordered_map<int,int>mp;
        for(auto it:nums)
            {
                mp[it]++;
            }
          long long g=(n/k);
        if(mp.size()==n) 
            return true;
        else{
            for(auto it:mp)
                {
                    if(it.second>g)
                    {
                        return false;
                    }
                }
        }
        return true;
    }
};

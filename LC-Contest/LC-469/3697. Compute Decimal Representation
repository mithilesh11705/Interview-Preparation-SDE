class Solution {
public:
    vector<int> decimalRepresentation(int n) {
        
        vector<int>ans;
        long long mul=1;
        while(n>0)
        {
            int d=n%10;
            if(d>0) ans.push_back(d*mul);
            n=n/10;
            if(n) mul*=10;
        }
        reverse(ans.begin(),ans.end());

        return ans;
    }
};
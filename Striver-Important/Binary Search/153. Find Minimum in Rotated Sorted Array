class Solution {
public:
    int findMin(vector<int>& nums) {
        int n=nums.size();
        int st=0,end=n-1;int k=INT_MAX;
        while(st<=end)
        {
            int m=st+(end-st)/2;
            
            if(nums[m]<nums[end])
            {
                end=m;
                k=min(k,nums[m]);
            }
            else
            {
                k=min(k,nums[st]);
                st=m+1;
                
            }

        }
        return k;




    }
};
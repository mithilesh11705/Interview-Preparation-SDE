class Solution {
public:
    int findPeakElement(vector<int>& nums) {
        
        int n=nums.size();
        int l=0,h=n-1;int m;
        while(l<h)
        {
             m=l+(h-l)/2;
            if(nums[m]>nums[m+1])
            h=m;
            else
            l=m+1;
        }
        return l;


    }
};
class Solution {
public:
    int search(vector<int>& nums, int target) {
        int  n=nums.size();
        int st=0,end=n-1;
        while(st<=end)
        {
            int m=st+(end-st)/2;

            if(nums[m]==target)
            return m;

            if(nums[st]<=nums[m])//left part  is sorted
            {
                if(nums[st]<=target && target<nums[m])
                end=m-1;
                else
                st=m+1;
            }
            else{
            if(nums[m]<target && target<=nums[end])
            {
                st=m+1;
            }
            else{
            end=m-1;
            }
            }
        }
        return -1;
    }
};
class Solution {
  public:
    int getSecondLargest(vector<int> &arr) {
        // code here
        int n=arr.size();
        
        int larg,slarg;
        larg=slarg=-1;
        
        for(int i=0;i<n;i++)
        {
            if(arr[i]>larg)
            {   
                slarg=larg;
                larg=arr[i];
            }
            else if(arr[i]<larg && arr[i]>slarg)
            {
                slarg=arr[i];
            }
        }
        return slarg;
    }
};